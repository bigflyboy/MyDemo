<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wangzhiyuan.mydemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".arouterdemo.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".volleydemo.MainActivity" />
        <activity android:name=".volleydemo.ImageLoaderActivity" />
        <activity android:name=".arouterdemo.ArouterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".arouterdemo.TestActivity" />
        <activity android:name=".annotationdemo.AnnotationActivity" />
        <activity android:name=".RecyclerActivity.RecyclerActivity">

        </activity>
    </application>

</manifest>